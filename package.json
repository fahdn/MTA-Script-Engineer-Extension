{
  "name": "mta-script-engineer",
  "displayName": "MTA Script Engineer",
  "description": "Autocompletion for MTA:SA exported functions",
  "publisher": "MTAScriptEngineer",
  "repository": {
      "type": "git",
      "url": "https://github.com/fahdn/MTA-Script-Engineer-Extension"
  },
  "icon": "images/icon.png",
  "version": "0.0.15",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:lua"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "lua",
        "path": "./snippets/snippets.json"
      }
    ],
    "configuration": {
        "title": "MTA SA Script Engineer",
        "properties": {}
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.3"
  }
}
